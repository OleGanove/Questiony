<div class="wrapper_skinny">
  <h1>Hello, my name is <%= @user.name %></h1>
  <h1>And I am a <%= get_gender(@user.gender) %></h1>
  <h1>And I am <%= @user.age %> years old </h1>
  <h2>My questions</h2>
  <% @user.questions.each do |question| %>
 	<h1><%= link_to question.question, question %></h1>

	<% if user_signed_in? && current_user == question.user %>
	<%= link_to "Delete", question_path(question), method: :delete, data: { confirm: "Are you sure?" }, class: "devise_links" %>
	<% end %>
  <% end %>
</div>